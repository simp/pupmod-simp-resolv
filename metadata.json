{
  "name": "simp-resolv",
  "version": "0.1.1",
  "author": "SIMP Team",
  "summary": "A SIMP Puppet module for managing client-side DNS settings",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-resolv",
  "project_page": "https://github.com/simp/pupmod-simp-resolv",
